{
    "name": "black/geo",
    "type": "library",
    "description": "PHP 5.4+ library to make working with Geo coordinates safer, easier, and fun",
    "keywords": ["libary", "black-project", "geo", "coordinates", "value-object"],
    "homepage": "http://www.lablackroom.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre 'pocky' Balmes",
            "email": "alexandre@lablackroom.com",
            "homepage": "http://www.lablackroom.com"
        }
    ],
    "autoload": {
        "psr-4": {
